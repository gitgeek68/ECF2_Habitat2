@model IEnumerable<MulhouseHabitat.Models.MHLogements>
<h2>Listes des logements à louer</h2>

<table class="table">
    
        <tr >
            
            <td id="bordure">
                @Html.DisplayNameFor(model => model.Type)
            </td>
            <td id="bordure">
                @Html.DisplayNameFor(model => model.StreetNumber)
            </td>
            <td id="bordure">
                @Html.DisplayNameFor(model => model.StreetName)
            </td>
            <td id="bordure">
                @Html.DisplayNameFor(model => model.PostalCode)
            </td>
            <td id="bordure">
                @Html.DisplayNameFor(model => model.City)
            </td>
            <td id="bordure">
                @Html.DisplayNameFor(model => model.NumberOfPieces)
            </td>
            <td id="bordure">
                @Html.DisplayNameFor(model => model.Size)
            </td>
            <td id="bordure">
                @Html.DisplayNameFor(model => model.Rented)
            </td>
            <td id="bordure">
                Actions
            </td>

        </tr>
    

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StreetNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StreetName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PostalCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.City)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NumberOfPieces)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Size)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Rented)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
